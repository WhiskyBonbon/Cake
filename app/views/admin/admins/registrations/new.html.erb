<div class="container">

  <div class="row">
    <h4 class="signup_title">管理者サインアップ</h4>
  </div>

  <div class="row">
    <div class="form-group">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "admin/admins/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label "メールアドレス" %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class:"form-control, col-lg-6" %>
  </div>

  <div class="field">
    <%= f.label "パスワード" %>
    <%= f.password_field :password, autocomplete: "new-password", class:"form-control, col-lg-6" %>
    <% if @minimum_password_length %>
    <em> (<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
  </div>

  <div class="field">
    <%= f.label "パスワード(確認)" %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password", class:"form-control, col-lg-6" %>
  </div>

  <div class="u_actions">
    <%= f.submit "サインアップ", class:"btn btn-primary btn-sm col-lg-4" %>
  </div>

  <div class="render_login">
    <%= render "admin/admins/shared/links" %>
  </div>

<% end %>
</div>

  </div>
</div>

